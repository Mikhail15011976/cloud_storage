{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^6.0.0",
    "@mui/material": "^6.0.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.9.0",
    "notistack": "^3.0.2",
    "nth-check": "^2.1.1",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.2",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "scripts": {
    "start": "react-scripts start --host 0.0.0.0",
    "build": "react-scripts build",
    "build:large": "node --max_old_space_size=4096 node_modules/.bin/react-scripts build",
    "build:prod": "set GENERATE_SOURCEMAP=false && react-scripts build",
    "build:analyze": "yarn build && npx serve -s build",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "memory-check": "node scripts/memory-test.js",
    "prebuild": "yarn run memory-check",
    "security-check": "npx audit-ci --moderate",
    "clean": "rimraf build node_modules/.cache",
    "clean:install": "yarn clean && yarn install",
    "clean:win": "rd /s /q build && rd /s /q node_modules\\.cache",
    "reset": "yarn clean:win && yarn install"
  },
  "devDependencies": {
    "rimraf": "^6.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
